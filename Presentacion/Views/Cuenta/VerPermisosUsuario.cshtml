}

@Html.Partial("_Espacio1")

<body>

    <div class="container contact_container">

        <h4>@Presentacion.Recursos.Recursos.CUENTA_VERPERMISOS_TITULO</h4>

        <table class="table">
            <tr>
                           
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Descripcion)
                    </td>
                   
                
                </tr>
            }

        </table>

        <div class="form-group">
            @Html.ActionLink("Volver", "ListarUsuarios", "Cuenta", null, new { @class = "btn btn-info btn-sm" })
